  <script>
     window.eventCallbackDelay = 0; // sec
     dataLayer = []

     String.prototype.babPadStart = function (len, char) {
         if ( this.length >= len ) {
             return this;
         } else {
             var needCount = len - this.length,
                 currentString = this;
             for ( var i = 0; i < needCount; i++  ) {
                 currentString = char + currentString;
             }
             return currentString;
         };
     };

     var toSEODataFormat = function( timeInstance ){
         return timeInstance.getFullYear().toString() + "-"
             + (timeInstance.getMonth() + 1).toString().babPadStart(2, '0') + "-"
             + timeInstance.getDate().toString().babPadStart(2, '0') + " "
             + timeInstance.getHours().toString().babPadStart(2, '0') + ":"
             + timeInstance.getMinutes().toString().babPadStart(2, '0') + ":"
             + timeInstance.getSeconds().toString().babPadStart(2, '0')
             + timeInstance.toTimeString().replace(/.+GMT([+-]\d+).+/, '$1');
     }

     var runEventCallbackDelay = function( callback ){
         if ( window.eventCallbackDelay && window.eventCallbackDelay > 0 ) {
             setTimeout(function(){
                 callback();
             }, window.eventCallbackDelay * 1000 );
         } else {
             callback();
         }
     }

     var dataLayerWrapper = function( data, pageEvent, pageEventData ){
         var defaultData = {
             dimension2:  toSEODataFormat(new Date()) //moment(new Date()).format('YYYY-MM-DD HH:mm:ssZZ')
         };

         for ( field in defaultData ) {
             data[field] = defaultData[field];
         }

         // build de dimensionS
         if ( pageEvent === 'load' ) {
             data['dimension3'] = 'English';
         }

         if ( data.event === 'instantPriceSearch' && pageEventData && pageEventData['departure_country']) {
             data['dimension4'] = pageEventData['departure_country'];
             data['metric10'] = pageEventData['passengers'];
         }

         return data;
     };
  </script>

