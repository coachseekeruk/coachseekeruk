  </script>

  <script type="text/javascript">
    var globalNotify = null, lastIndexItem = -1;

    var showBookingRecToast = function( content ){
        var objImg = new Image();
        objImg.src = content.icon;

        objImg.onload = function() {
            globalNotify = $.notify( content, { placement: {
                        from: "bottom",
                        align: "left"
                    },
                    type: "popup",
                    offset: 20,
                    spacing: 10,
                    delay: 5000,
                    timer: 1000,
                    icon_type: 'image',
                    animate: {
                        enter: 'animated fadeInDown',
                        exit: 'animated fadeOutUp'
                    },
                    template: $('#notify-template').html()
                }
            );
        }
    }

    var getBookingData = function( ) {
      if ( toastData ) {
          var currentIndex = Math.floor(Math.random() * 100);
          if ( lastIndexItem !== currentIndex ) {
              lastIndexItem = currentIndex;
          }
          if ( toastData[lastIndexItem] ) {
              showBookingRecToast( toastData[lastIndexItem].item );
          }
      } else {
          $.ajax({
              url: '/en/api/get-toast-card.json',
              type: 'GET',
              success: function (response) {
                  if (response.success) {
                      showBookingRecToast(response.item);
                  }
              }
          });
      }
    };

    var cicleBookingRecToast = function( ) {
        getBookingData();
        setInterval(function(){
            getBookingData();
        }, 60000);
    }

    $(window).load(function( ){
        if ( window.toastData && toastData.length > 0 ) {
            $('#toast-data-container').remove();
        }

        setTimeout(function( ){
            cicleBookingRecToast();
        }, 30000);
    });
  </script>


</body>
</html>

