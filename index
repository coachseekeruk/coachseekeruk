<script>

  var showRedirectModal = function(callback, international, closedCallback){
        var modalElem = $('#quote-redirect-modal-new'),
            redirectButton = $('#quota-redirect-button'),
            searchButton = $('#search-redirect-button');

        modalElem.unbind();
        redirectButton.unbind('click');
        searchButton.unbind('click');

        if(typeof analytics != 'undefined'){
            analytics.track('Redirect to quote modal', {type: international ? 'manual' : 'automatic'});
        }

        if( typeof(saveToBigData) === 'function' ){
            saveToBigData('Redirect to quote modal', {
                type: international ? 'manual' : 'automatic'
            });
        }

        if(international){
            $( '#qm-headline' ).html( 'Your international trip requires a little bit more love.' );
            $( '#qm-text1' ).html( 'You will need to make a quote request if your coach trip is international or exceeds the allowed daily driving hours.' );
            $( '#qm-text2' ).html( 'Click below and you will be done in two minutes.' );
            $( '#quota-redirect-button' ).html( 'Get a quote for your international trip within 24-48 hours' );
            $( '#search-redirect-button' ).html( 'Search for another local trip' );
        } else {
            $( '#qm-headline' ).html( 'Your trip requires a little bit more love.' );
            $( '#qm-text1' ).html( 'You will need to make a quote request if your coach trip crosses borders or exceeds the allowed daily driving hours.' );
            $( '#qm-text2' ).html( 'We will provide you with the quotes on your requested trip after you submit the form online. ' );
            $( '#quota-redirect-button' ).html( 'Submit your quote request in less than 2 minutes' );
            $( '#search-redirect-button' ).html( 'Search for another shorter trip' );
        }

        if ( typeof( dataLayer )  !== 'undefined') {
            dataLayer.push( dataLayerWrapper( {
                'event': 'quotePrompt',
                'eventCategory': 'Quote Funnel',
                'eventAction': 'RFQ Modal Appear',
                'eventLabel': international ? 'Pre-search' : 'Post-search',
                'eventValue': undefined
            }, 'click' ) );
        }


        modalElem.modal({ keyboard: false });

        setTimeout( function( ) {
            if ( typeof( hideSearchLoader ) === 'function' ) {
                hideSearchLoader();
            }

            if ( typeof( hideMask ) === 'function' ) {
                hideMask();
            }
        }, 500 );

        if ( typeof( closedCallback ) === 'function' ) {
            modalElem.on( 'hidden.bs.modal', function () {
                closedCallback( this );
            });
        }


      $('#quota-redirect-button').click(function(){
            if ( typeof( dataLayer )  !== 'undefined') {
                dataLayer.push( dataLayerWrapper( {
                    'event' : 'quotePromptAction',
                    'eventCategory' : 'Quote Funnel',
                    'eventAction' : 'Clicked RFQ Button',
                    'eventLabel' : 'Post-search',
                    'eventValue': undefined,
                    'eventCallback': function(){
                        runEventCallbackDelay(function(){
                            if( callback ) {
                                callback();
                            } else {
                                location.href = '/en/myprofile/ab/quotas/new?redirected=true';
                            }
                            modalElem.modal('hide');
                        });
                    }
                }, 'click' ) );
            } else {
                if(callback) callback();
                modalElem.modal('hide');
            }

        });

      $('#search-redirect-button').click( function( ) {
          var _gtmCallback = function( label, callback ) {
              dataLayer.push( dataLayerWrapper( {
                  'event' : 'quotePromptAction',
                  'eventCategory' : 'Quote Funnel',
                  'eventAction' : 'Clicked Search Again Button',
                  'eventLabel' : label,
                  'eventValue': undefined,
                  'eventCallback': runEventCallbackDelay( function(){ callback(); })
              }, 'click' ));
          };

          var eventLabel = international ? "Pre-search" : "Post-search";
          _gtmCallback( eventLabel, function( ) {
              modalElem.modal( 'hide' );
          } );

      } );
    }

</script>


